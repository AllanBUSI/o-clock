<%- include('partials/header') %>

<h1>Résultats de recherche</h1>

<h3>Par élément</h3>

<% if(results.length){%>
    <ul>
      <% for(let result of results) {%>
        <li><a href="/card/<%= result.id %>">
            <%= result.name %>
          </a></li>
        <%}%>
    </ul>
    <% } else { %>
      <p>Aucun résultat</p>
      <% } %>



<%- include('partials/footer') %>